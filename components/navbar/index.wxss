/**index.wxss**/

@import '/lib/iconfont/iconfont.wxss';
.navbar {
  position: fixed;
  top: 0;
  left: 0;
  color: #fff;
  overflow: hidden;
  padding: 50rpx 20rpx 0;
  width: 100%;
  height: 68rpx;
  background-color: #42BD56;
}

.navbar-height {
  height: 118rpx;
}

.navbar-icon {
  position: relative;
  z-index: 10;
  float: left;
  line-height: 68rpx;
  width: 80rpx;
  text-align: center;
  font-size: 40rpx;
}

.navbar-title {
  position: absolute;
  z-index: 10;
  top: 50rpx;
  left: 0;
  text-align: center;
  width: 100%;
  line-height: 68rpx;
  font-size: 32rpx;
}
